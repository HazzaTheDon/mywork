
public class loops {
	public static void main(String[] args) {
		int MoneyCount =0;
		boolean NotEnoughMoney = true;
		
		while (NotEnoughMoney)
			System.out.println("'Another one' in DJ Khaled's voice ");
			MoneyCount++;
			if (MoneyCount<10)
				NotEnoughMoney= false;
			System.out.println("I was broke but i'm living in the hills now");
			
			for(int money=0; money<10; money++) {
				System.out.println("'another one' in DJ Khaled's voice");
				
			}
			System.out.println("I was broke but i'm living in the hills now");

	}}