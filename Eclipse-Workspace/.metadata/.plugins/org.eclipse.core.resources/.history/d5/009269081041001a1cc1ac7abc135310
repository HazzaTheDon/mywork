
public class HelpForDev {
	public static void main(String[] args) {
	int day = 5;
	switch (day) {

	case 1:
	System.out.println("I've been on the low");
	break;
	case 2:
	System.out.println("I been taking my time,");
	break;
	case 3:
	System.out.println("I been taking my time");
	break;
	case 4:
	System.out.println("It feel like my life ain't minet");
	break;
	case 5:
	System.out.println("Who can relate?");
	break;
	case 6:
	System.out.println("I've been on the low");
	break;
	case 7:
	System.out.println("I been taking my time");
	break;
	default:
	System.out.println("You need to listen to 1-800-273-8255");
	break;

	}}}
	
