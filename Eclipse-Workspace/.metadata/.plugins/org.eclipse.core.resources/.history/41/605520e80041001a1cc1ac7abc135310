
public class LearningMethods {
	public static void main(String[] args) {
		IsDivisible(69,24);}
		public static void IsDivisible(int num,int divider) {
			System.out.println(num %divider == 0);
			int a=10;
			int b =3;
			System.out.println("Is" + a +"divisible by" + b +":" + isDivisible(a,b);
		}
			 
	
